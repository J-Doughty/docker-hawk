{
  "name": "docker-hawk",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "npm run tauri dev",
    "tauri:build": "npm run tauri build",
    "tauri:install": "sudo dpkg -i ./src-tauri/target/release/bundle/deb/tauri-app_0.1.0_amd64.deb",
    "tauri:uninstall": "sudo apt remove tauri-app",
    "prettier:check": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "lint:check": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "check": "npm run prettier:check && npm run lint:check",
    "fix": "npm run prettier:fix && npm run lint:fix"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^8.8.0",
    "@mui/x-data-grid-generator": "^8.8.0",
    "@tanstack/react-router": "^1.125.6",
    "@tanstack/react-router-devtools": "^1.125.6",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-shell": "^2.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@tanstack/router-plugin": "^1.125.6",
    "@tauri-apps/cli": "^2",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.37.0",
    "vite": "^6.0.3"
  }
